    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
    <children>
        <TableView fx:id="tableId" AnchorPane.topAnchor="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
            <columns>
                <TableColumn fx:id="typeCol" prefWidth="150.0" text="Type" />
                <TableColumn fx:id="sujetCol" prefWidth="150.0" text="Sujet" />
                <TableColumn fx:id="dateCol" prefWidth="150.0" text="Date" />
            </columns>
        </TableView>
    </children>
</AnchorPane>
